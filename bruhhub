      local Jump = true  

game.UserInputService.JumpRequest:Connect(function()
    if Jump then
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid:ChangeState("Jumping")
        end
    end
end)	
